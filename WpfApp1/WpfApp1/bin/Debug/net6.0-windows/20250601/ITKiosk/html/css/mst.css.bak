.ticketWeek {
    /* width: 880px; */
    /* height: 30px; */
    background-color: #fff;
    border-radius: 20px;
    margin-bottom: 5px;
}

.ticketWeek li {
    display: inline-block;
    width: 120px;
    text-align: center;
    font-weight: bold;
}

.ticketDate {
    /* width: 880px; */
    /* height: 50px; */
    padding-top: 3px;
    background-color: #fff;
    border-radius: 20px;
    margin-bottom: 5px;
}

.ticketDate li {
    display: inline-block;
    width: 120px;
    text-align: center;
    padding: 5px;
    color: #000000;
    font-weight: bold;
}

.calendarL {
    position: relative;
}

.calendarL::after {
    content: "";
    border: solid black;
    border-width: 0px 3px 3px 0;
    display: inline-block;
    padding: 3px;
    height: 12px;
    width: 12px;
    position: absolute;
    top: 50%;
    margin-top: -6px;
    left: -6px;
    transform: rotate(135deg);
}

.calendarR {
    position: relative;
}

.calendarR::after {
    content: "";
    border: solid black;
    border-width: 0px 3px 3px 0;
    display: inline-block;
    padding: 3px;
    height: 12px;
    width: 12px;
    position: absolute;
    top: 50%;
    margin-top: -6px;
    left: -6px;
    transform: rotate(-45deg);
}

.ou {
    color: #999999 !important;
    font-weight: normal !important;
}

.pi {
    color: #fff;
    background-color: #FF2D55;
    border-radius: 100px;
    pointer-events: none;
}

.ticketSession {
    width: 980px;
    /* height: 90px; */
    padding-top: 3px;
    margin-bottom: 10px;
    display: flex;
}

.sessionItem {
    font-weight: bold;
    font-size: 18px;
    text-align: center;
    width: 200px;
    height: 90px;
    background-color: #fff;
    border: 2px #FF2D55 solid;
    border-radius: 10px;
    overflow: hidden;
    margin: 0 5px;
}

.sessionTitle {
    color: #fff;
    background-color: #FF2D55;
}

.sessionItem2 {
    font-weight: bold;
    font-size: 18px;
    text-align: center;
    width: 200px;
    height: 90px;
    background-color: #fff;
    border-radius: 10px;
    overflow: hidden;
    margin: 0 5px;
}

.sessionTitle2 {
    color: #fff;
    background-color: #ccc;
}


.sessionItemSp {
    font-weight: bold;
    font-size: 18px;
    text-align: center;
    width: 200px;
    height: 90px;
    background-color: #fff;
    border-radius: 10px;
    overflow: hidden;
    margin: 0 5px;
    pointer-events: none;
}

.sessionItemSp .sessionTitleSp {
    color: #fff;
    background-color: #ccc;
}

.sessionItemSp.sell .sessionTitleSp {
    color: #fff;
    background-color: #FF2D55;
}

.sessionItemSp.sell * {
    pointer-events: all;
}

.sessionItemSp.sell.pick {
    border: 2px #FF2D55 solid;
}

.sessionItemSp.sell.pick * {
    pointer-events: none;
}


.sessionItemSp .sessionCont {
    padding: 5px;
    line-height: 0.5em;
}

.sessionItemSp .sessionCont p {
    margin-top: 10px;
    font-size: 20px;
}

.ticketExplaination {
    font-size: 14px;
    color: #999;
}

.priceQuantityG {
    width: 380px;
    font-size: 28px;
    display: flex;
}

.ticketPrice {
    width: 150px;
    font-weight: bold;
    text-align: right;
    margin-left: 10px;
}

.choseTicketQuantityG {
    margin-left: 30px;
}

.choseTicketQuantityG i {
    font-size: 35px;
}

.ticketQuantity {
    display: inline-block;
    text-align: center;
    width: 50px;
}


.keyboard__num {
    width: 105px;
    height: 105px;
    border-radius: 20px;
    border: 1px solid #6c757d;
    margin: 0.5rem 0;
    color: #6c757d;
    font-weight: bold;
    font-size: 32px;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    background-color: white;
}


.extend-via-pseudo-elem {
    position: relative;
}

.extend-via-pseudo-elem::before {
    content: '';
    position: absolute;
    top: -5px !important;
    right: -20px !important;
    bottom: -5px !important;
    left: -20px !important;
}