	<THERMAL_TEXT>列印日期:{{PrintDate}}</THERMAL_TEXT>
	<THERMAL_TEXT>\r\n\r\n\r\n\r\n</THERMAL_TEXT>
    </THERMAL_PAGE>
  </THERMAL_PRINT>
  
