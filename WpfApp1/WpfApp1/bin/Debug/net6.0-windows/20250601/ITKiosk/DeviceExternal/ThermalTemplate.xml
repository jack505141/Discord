  <THERMAL_PRINT id="Refund">
    <THERMAL_PAGE>
    <THERMAL_TEXT alignment="center">電子發票銷貨退回、進貨退出或折讓證明單\r\n</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">{{RefundDate}}\r\n</THERMAL_TEXT>
