<THERMAL_TEMPLATE>  
  <THERMAL_PRINT id="Ticket">
    <THERMAL_PAGE>
	<THERMAL_BITMAP file="C:\ITKiosk\html\img\Ticket.bmp" alignment="center" size="400"></THERMAL_BITMAP>
	<THERMAL_TEXT>\r\n</THERMAL_TEXT>
    <THERMAL_QRCODE alignment="center">{{redeemCode}}</THERMAL_QRCODE>
    <THERMAL_TEXT alignment="center">\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center">{{productName}}\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center" bold='true' wide_character="2">{{ticketTypeName}}\r\n\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center" bold='true'>{{paymentSettingListName}} NT${{salePriceStr}}\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center">{{printDate}}\r\n</THERMAL_TEXT>
    <THERMAL_TEXT>{{generateStr}}</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">----------------------------------\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>{{memoStr}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">{{ticketNumber}} | {{redeemCode}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">{{now}}印[序{{serialNum}}]{{recall}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>\r\n\r\n\r\n\r\n</THERMAL_TEXT>
    </THERMAL_PAGE>
  </THERMAL_PRINT>
  
  <THERMAL_PRINT id="Payment">
    <THERMAL_PAGE>
	<THERMAL_BITMAP file="C:\ITKiosk\html\img\Payment.bmp" alignment="center" size="400"></THERMAL_BITMAP>
    <THERMAL_TEXT alignment="center">\r\n入場憑證:\r\n</THERMAL_TEXT>
    <THERMAL_QRCODE alignment="center">{{paymentRedeemCode}}</THERMAL_QRCODE>
	<THERMAL_TEXT alignment="center">\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center">使用期限 : 以現場公告為準\r\n</THERMAL_TEXT>
    <THERMAL_TEXT>{{generateStr}}</THERMAL_TEXT>
	<THERMAL_TEXT>----------------------------------\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>{{memoStr}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>----------------------------------\r\n</THERMAL_TEXT>
    <THERMAL_TEXT alignment="center">交易明細\r\n</THERMAL_TEXT>
    <THERMAL_TEXT>訂單編號 : {{paymentNumber}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>交易日期 : {{paidDate}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>出票設備 : {{saleDeviceCode}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>付款方式 : {{paymentSettingListName}} ${{paymentSettingListPrice}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>買方統編 : {{Unified}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>發票號碼 : {{invoiceNumber}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>載具類型 : {{invoiceDeviceTypeName}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>==================================</THERMAL_TEXT>
	<THERMAL_TEXT>  品項                 $單價/數量  </THERMAL_TEXT>
	<THERMAL_TEXT>==================================</THERMAL_TEXT>
	<THERMAL_TEXT>{{DgenerateStr}}</THERMAL_TEXT>
	<THERMAL_TEXT>----------------------------------</THERMAL_TEXT>
	<THERMAL_TEXT>合計 {{totalCount}} 項 | 應收總額 ${{totalPrice}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>{{taxInfo}}</THERMAL_TEXT>
	<THERMAL_TEXT>==================================\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>{{kioskInvoice}}</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">{{paymentNumber}} | {{paymentRedeemCode}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT alignment="center">{{now}}印[序{{serialNum}}]{{recall}}\r\n</THERMAL_TEXT>
	<THERMAL_TEXT>\r\n\r\n\r\n\r\n</THERMAL_TEXT>
    </THERMAL_PAGE>
  </THERMAL_PRINT>
  
