<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <appSettings>    
      <!--多久巡一次job。單位：秒數的整數。-->
      <add key="JobInterval" value="600"/>
      <!--保留多少天的log檔，需為數字。單位：天-->
      <add key="KeepLogDay" value="3"/>
    </appSettings>
</configuration>